package com.ayusha.general.services.data.model;

import java.util.List;

/**
 * 
 * @author author
 * 10-Aug-1029
 * State data
 *
 */
public class StatesDataModel {
	
	/** id **/
	private int id;
	
	
	
	/** state list **/
	private List<StatesDataModel> lstCitiesDataModel;
	
	/**
	 * default constructor
	 */
	public StatesDataModel() {
		
	}

	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}

	/**
	 * @return the lstCitiesDataModel
	 */
	public List<StatesDataModel> getLstCitiesDataModel() {
		return lstCitiesDataModel;
	}

	/**
	 * @param lstCitiesDataModel the lstCitiesDataModel to set
	 */
	public void setLstCitiesDataModel(List<StatesDataModel> lstCitiesDataModel) {
		this.lstCitiesDataModel = lstCitiesDataModel;
	}
}
